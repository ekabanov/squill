package squill.query.select;

import squill.query.Expression;

/**
 * Expression that can be used in SELECT-clause.
 * <p>
 * @see Expression
 * @see SelectExpression
 */
public interface SelectExpression<T> extends Expression<T> {

  /**
	 * Returns the class representing the expression type.
	 * 
	 * @return the class representing the expression type.
	 */
	Class<T> getTableType();

}
